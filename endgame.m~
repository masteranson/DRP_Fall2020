function [xval,num_solutions] = endgame(jac_eval,  xval,  tval, dt)

condition_threshold = 10^5;
num_solutions = length(xval);

for counter = 1:length(xval) %Truncating tracking solutions based on predictor condition number
    condition_number = cond(jac_eval(xval(counter,1),xval(counter,2),xval(counter,3),tval+dt));
    if condition_number > condition_threshold
        num_solutions = num_solutions - 1;
        fprintf('Removed Tracking Solution with Condition Number %d\n',condition_number);
    else
        xval_new(counter,:) = xval(counter,:);
    end
end

xval = 
end